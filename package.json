{
  "name": "node-videostream-api",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AdrianLThomas/node-videostream-api"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "tsc -p ./src",
    "prestart": "npm run lint",
    "watch": "tsc -w -p ./src",
    "debug": "nodemon --watch ./dist --inspect=0.0.0.0:5858 --nolazy ./dist/index.js",
    "docker-debug": "docker-compose -f docker-compose.debug.yml up",
    "start": "node ./dist/index.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "mocha -r ts-node/register './src/**/*.spec.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.5",
    "@types/sinon": "^4.3.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.0.5",
    "nodemon": "^1.17.3",
    "sinon": "^4.5.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typemoq": "^2.1.0",
    "typescript": "^2.8.1"
  },
  "main": "./dist/index.js"
}
